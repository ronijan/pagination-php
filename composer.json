{
  "name": "ronijan/pagination-php",
  "description": "Simple Pagination in SQL and PHP.",
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "Huseyin Khalil",
      "email": "huseyinkhalil@web.de"
    }
  ],
  "require": {
    "php": "^7.3",
    "ronijan/backup-db": "dev-master",
    "ext-mysqli": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Ronijan\\Pagination\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/ExampleTest.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
